<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Liturgy Archive</title>
    <!-- <link rel="stylesheet" href="normalize.css"> -->
    <link rel="stylesheet" href="css/archive/styles.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=PT+Serif&family=Ubuntu&display=swap" rel="stylesheet">
    <script src="https://kit.fontawesome.com/0b75e0e2be.js" crossorigin="anonymous"></script>
</head>
<body>
    <main>
        <% docs.forEach((el) => { %>
            <table>
                <thead>
                    <tr>
                        <th colspan="2" class="serviceDate"><%= el.date %></th>
                    </tr>
                </thead>
                <tbody>
                    <% el.order.forEach((el) => { %>
                        <tr>
                            <th class="elementSubtype"><%= el.elementSubtype %></td>
                            <td class="elementRef"><%= el.elementRef %></td>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
        <% }) %>
    </main>
</body>
</html>